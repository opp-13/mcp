{
  "$schema": "https://raw.githubusercontent.com/aws/amazon-q-developer-cli/refs/heads/main/schemas/agent-v1.json",
  "name": "mcp-dev",
  "description": "mcp 개발용 Agent",
  "prompt": "You are a Assistant of a dev team who make a MCP Server with MCP Python SDK. When generating code, please always reference ~/mcp/dev/* and must read and follow the rules in /.amazonq/rules/**/*.md.",
  "mcpServers": {
    "awslabs.core-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.core-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "aws-foundation": "true",
        "solutions-architect": "true"
      },
      "autoApprove": [],
      "disabled": false
    },
    "awslabs.aws-diagram-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.aws-diagram-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "autoApprove": [],
      "disabled": false
    },
    "awslabs.code-doc-gen-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.code-doc-gen-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "awslabs.aws-api-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.aws-api-mcp-server@latest"
      ],
      "env": {
        "AWS_REGION": "us-east-1"
      },
      "disabled": false,
      "autoApprove": []
    },
    "aws-knowledge-mcp-server": {
      "command": "uvx",
      "args": [
        "mcp-proxy@latest",
        "--transport",
        "streamablehttp",
        "https://knowledge-mcp.global.api.aws"
      ]
    },
    "awslabs.aws-documentation-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.aws-documentation-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_DOCUMENTATION_PARTITION": "aws"
      },
      "disabled": false,
      "autoApprove": []
    },
    "well-architected-security-mcp-server": {
      "command": "uvx",
      "args": [
        "--from",
        "awslabs.well-architected-security-mcp-server@latest",
        "well-architected-security-mcp-server"
      ],
      "env": {
        "AWS_PROFILE": "your-aws-profile",
        "AWS_REGION": "your-aws-region",
        "FASTMCP_LOG_LEVEL": "ERROR"
      }
    },
    "strands": {
      "command": "uvx",
      "args": ["strands-agents-mcp-server"]
    },
    "github": {
      "command": "/path/to/github-mcp-server",
      "args": ["stdio"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "{GIT_TOKEN}}"
      }
    }

  },
  "tools": [
    "*"
  ],
  "toolAliases": {
    "read": "fs_read",
    "write": "fs_write",
    "run": "execute_bash"
  },
  "allowedTools": [
    "fs_read",
    "fs_write",
    "execute_bash",
    "use_aws",
    "prompt_understanding"
  ],
  "resources": [
    "file://AmazonQ.md",
    "file://README.md",
    "file://.amazonq/rules/**/*.md",
    "file://~/mcp/dev/**"
  ],
  "toolsSettings": {},
  "useLegacyMcpJson": true
}